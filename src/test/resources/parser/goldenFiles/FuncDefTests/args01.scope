ScopeEnvironment
  Scope null Module
    Varnames: arg
    Symbols: 
      arg: [DefParam, Local]
      fn: [DefLocal, Local]
    Scope fn Function
        Flags: [HasVarArgs]
        Varnames: a, b, k1, k2, arg
        Symbols: 
            a: [DefParam, Local]
            b: [DefParam, Local]
            k1: [DefParam, Local]
            k2: [DefParam, Local]
            arg: [DefParam, Local]
            sum: [Use, GlobalImplicit]